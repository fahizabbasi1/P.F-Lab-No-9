#include <iostream>
using namespace std;

int main() {
   
    int arr[3][3];
    int sum = 0;

  
    cout << "Enter the values for a 3x3 matrix:" << endl;
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            cout << "Enter value for position [" << i << "][" << j << "]: ";
            cin >> arr[i][j];
        }
    }


    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            sum += arr[i][j];
        }
    }

    cout << "\nThe sum of all elements in the 3x3 matrix is: " << sum << endl;

    return 0;
}
